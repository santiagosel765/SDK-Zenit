  },
  "keywords": [
    "zenit",
    "sdk",
    "typescript",
    "react"
  ],
  "author": "",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "react": "^18.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.0",
    "tsup": "^8.0.0",
    "typescript": "^5.4.0",
    "react": "^18.0.0"
  }
}
